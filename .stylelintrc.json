{
  "plugins": [
      "./plugins/declaration-property-value-to-variable.js"
  ],
  "rules": {
    "ss-style-plugin/declaration-property-value-to-variable": [{ 
      "color": {
        "green": "$abc",
        "#fff": "$white",
        "#262626": "$text-primary",
          "#aaa": "$text-weak",
          "#AAAAAA": "$text-weak",
          "#999": "$text-minor",
          "#999999": "$text-minor",
          "#ccc": "$text-disabled",
          "#CCCCCC": "$text-disabled",
          "#666": "$text-major",
          "#666666": "$text-major"
      }
    }, {
      "import": [
        {
          "keyword": ["colors.scss", "variable/_colors"],
          "file": "@import 'variable/_colors.scss';"
        }
      ]
    }]
  }
}